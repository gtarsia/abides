{
  "name": "abides",
  "version": "0.7.0",
  "description": "elegant javascript object validation",
  "main": "index.js",
  "scripts": {
    "fix": "eslint --fix lib '**/*.js'",
    "esw": "esw --watch --color '**/*.js'",
    "test": "nyc ava",
    "test:watch": "ava --watch"
  },
  "files": [
    "lib",
    "*.js"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:gtarsia/abides.git"
  },
  "author": "gtarsia <guidotarsia@gmail.com>",
  "license": "MIT",
  "keywords": [
    "validation",
    "object"
  ],
  "devDependencies": {
    "codecov": "^3.4.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-ava": "^6.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsdoc": "^4.8.3",
    "eslint-watch": "^5.0.1",
    "nyc": "^14.1.1"
  },
  "ava": {
    "files": [
      "**/*.test.js"
    ],
    "require": [
      "esm"
    ]
  },
  "dependencies": {
    "ava": "^2.3.0",
    "esm": "^3.2.22",
    "fromentries": "^1.1.0",
    "lodash": "^4.17.11"
  }
